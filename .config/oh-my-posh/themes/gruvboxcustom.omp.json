{
  "palette": {
    "red"    : "#CC241d",
    "green"  : "#98971a",
    "yellow" : "#D79921",
    "yellow2": "#FABD2F",
    "orange" : "#D65D0E",
    "blue"   : "#83A598",
    "blue2"  : "#458588",
    "gray2"  : "#928374",
    "gray"   : "#A89984",
    "fg4"    : "#A89984",
    "fg0"    : "#FBF1C7",
    "fg_1"   : "#1D2021",
    "bg2"    : "#504945",
    "bg0"    : "#282828",
    "bg1"    : "#3C3836",
    "bg4"    : "#7C6F64",
    "aqua"   : "#689D6A",
    "purple" : "#D3869B"     
  },
  "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "p:green",
          "template": "⠀"
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "p:yellow2",
          "template": "⠀"
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "p:red",
          "template": "⠀"
        },
        {
          "foreground": "p:fg_1",
          "background": "p:gray",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }}",
          "style": "diamond",
          "trailing_diamond": "",
          "type": "session"
        },
        {
          "background": "p:yellow",
          "foreground": "p:fg_1",
          "trailing_diamond": "",
          "template": "  {{ .Path }} ",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "leading_diamond": "<transparent>\ue0b0</>",
          "trailing_diamond": "",
          "background": "p:blue",
          "foreground": "p:fg_1",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
            "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
            "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}\uf00d {{ else }}\uf42e {{ end }}",
          "foreground": "p:fg_1",
          "background": "p:green",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#CC241d {{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2500❯❯",
          "foreground": "p:blue",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}#ff3030{{end}}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
