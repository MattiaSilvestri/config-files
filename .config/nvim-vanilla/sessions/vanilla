let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/.dotfiles/.config/nvim-vanilla
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/blink-cmp.lua
badd +32 ~/.dotfiles/.config/nvim-vanilla/init.lua
badd +23 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/mason.lua
badd +2 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/ui/buffers.lua
badd +349 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/ui/heirline.lua
badd +18 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/ui/themes.lua
badd +59 ~/.local/share/nvim/lazy/AstroNvim/lua/astronvim/plugins/heirline.lua
badd +236 ~/.local/share/nvim/lazy/astroui/lua/astroui/config.lua
badd +47 ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/lsp.lua
argglobal
%argdel
edit ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/lsp.lua
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
wincmd =
argglobal
setlocal foldmethod=manual
setlocal foldexpr=v:lua.vim.treesitter.foldexpr()
setlocal foldmarker={{{,}}}
setlocal foldignore=#
setlocal foldlevel=0
setlocal foldminlines=1
setlocal foldnestmax=20
setlocal foldenable
silent! normal! zE
let &fdl = &fdl
let s:l = 47 - ((43 * winheight(0) + 22) / 44)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 47
normal! 0
wincmd w
argglobal
if bufexists(fnamemodify("~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/mason.lua", ":p")) | buffer ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/mason.lua | else | edit ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/mason.lua | endif
if &buftype ==# 'terminal'
  silent file ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/mason.lua
endif
balt ~/.dotfiles/.config/nvim-vanilla/lua/plugins/coding/lsp.lua
setlocal foldmethod=manual
setlocal foldexpr=v:lua.vim.treesitter.foldexpr()
setlocal foldmarker={{{,}}}
setlocal foldignore=#
setlocal foldlevel=0
setlocal foldminlines=1
setlocal foldnestmax=20
setlocal foldenable
silent! normal! zE
let &fdl = &fdl
let s:l = 23 - ((21 * winheight(0) + 22) / 44)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 23
normal! 0
wincmd w
2wincmd w
wincmd =
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
